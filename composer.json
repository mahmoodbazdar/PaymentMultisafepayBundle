    "type": "symfony-bundle",
    "description": "A Symfony2 Bundle that provides access to the MultiSafepay API. Based on JMSPaymentCoreBundle.",
    "keywords": ["payment", "multisafepay"],
    "homepage": "https://github.com/ruudk/PaymentMultisafepayBundle",
    "license": "Apache2",
    "authors": [
        {
            "name": "Ruud Kamphuis",
            "email": "ruudk@mphuis.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.0",
        "jms/payment-core-bundle": "~1.0",
        "omnipay/multisafepay": "~2.0",
        "psr/log": "~1.0"
    },
    "autoload": {
        "psr-0": { "Ruudk\\Payment\\MultisafepayBundle": "" }
    },
    "target-dir": "Ruudk/Payment/MultisafepayBundle"
